@model ProjectManagementApplication.Models.User

<div>
    <h1 class="text-center">TOXA</h1>
</div>

<div>
    <h1 class="text-center">Admin Dashboard</h1>
</div>

<div class="app-container">
    <table class="table table-striped table-hover table-striped">
        <thead>
            <tr>
                <th>ID</th>
                <th>Name</th>
                <th>Email</th>
                <th>Phone No.</th>
                <th>Type</th>
                <th>Password</th>
                <th>Image URL</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>@Model.Id</td>
                <td>@Model.UserName</td>
                <td>@Model.UserEmail</td>
                <td>@Model.Phone</td>
                <td>@Model.UserType</td>
                <td>@Model.Password</td>
                <td>@Model.ImageURL</td>

            </tr>
        </tbody>
    </table>
</div>